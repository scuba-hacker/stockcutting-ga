\par   case 27: if(conf==90) r = 1.314; else if(conf==95) r = 1.703 ; else r = 
\par 2.473; break;
\par   case 28: if(conf==90) r = 1.313; else if(conf==95) r = 1.701 ; else r = 
\par 2.467; break;
\par   case 29: if(conf==90) r = 1.311; else if(conf==95) r = 1.699 ; else r = 
\par 2.462; break;
\par   default: if(conf==90) r = 1.282; else if(conf==95) r = 1.645 ; else r = 
