\par   \}
\par return(r);
\par \}
\par int get_input(FILE *f, double *arr)
\par \{
\par   double v;
\par   n = 0;
\par   while(getdouble(f, &v, FALSE)!=0) arr[n++]=v;
\par \tab return 0;
\par \}
\par /*
\par  *
\par  * Peter Ross' getint etc functions.
\par  */
\par /***************************************************************/
\par /* Get the next number from the input: put it in the location  */
\par /* addressed by second argument. This function returns 0 on    */
\par /* EOF. If stopateol is true, it returns -1 when it hits \\n    */
\par /* (after which some other procedure has to read past the \\n), */
\par /* otherwise it continues looking for the next number.         */
\par /* A number has an optional sign, perhaps followed by digits,  */
\par /* perhaps followed by a decimal point, perhaps followed by    */
\par /* more digits. There must be a digit somewhere for it to count*/
\par /* as a number. So it would read any of:                       */
\par /*  -.5                                                        */
\par /*  -0.5                                                       */
\par /*  -.5.7                                                      */
\par /* as minus-a-half. In the last case, it would read .7 next    */
\par /* time around.                                                */
\par /*   There doesn't seem to be a neat and reliable way to do    */
\par /* all this, including stopateol, using scanf?                 */
\par /***************************************************************/
\par double 
\par getdouble (FILE *file, double *valaddr, int stopateol)
\par \{
\par   int c;
\par   int found = FALSE, indecimal = FALSE;
\par   int sign = +1;
\par   double n = 0.0, p = 1.0;
\par   /* First find what looks like start of a number - the first digit. */
\par   /* And note any sign and whether we just passed a decimal point.   */  
\par   do \{
\par     c = fgetc(file);
\par     if(c == EOF) return (0);
\par     else if(stopateol && c =='\\n') return(-1);
\par     else if(c == '+' || c == '-') \{
\par       sign = (c == '+')? +1 : -1;
\par       c = fgetc(file);
\par       if(c == EOF) return (0);
\par       else if(stopateol && c =='\\n') return(-1);
\par     \}
\par     if(c == '.') \{
\par       indecimal = TRUE;
\par       c = fgetc(file);
\par       if(c == EOF) return (0);
\par       else if(stopateol && c =='\\n') return(-1);
\par     \}
\par     if(c >= '0' && c <= '9') \{
\par       found = TRUE;
\par     \} else \{
\par       sign = +1;
\par       indecimal =  FALSE;
\par     \}
\par   \} while(!found);
\par   /* Now we've got digit(s) ... */
\par   do \{
\par     n = 10.0*n + c - '0';
\par     p = 10.0*p;
\par     c = fgetc(file);
\par     if((c < '0') || (c > '9')) \{
\par       found = FALSE;
\par       /* We've run out. If we already saw a decimal point, return now */
\par       if(indecimal) \{
\par         if(c != EOF) ungetc(c,file);
\par         *valaddr =  sign * n/p;
\par         return(1);
\par       \} else p = 1.0;
\par     \}
\par   \} while(found);
\par   /* We ran out and we didn't see a decimal point, so is this a decimal? */
\par   if(c != '.') \{
\par     /* No, give it back to caller */
\par     if(c != EOF) ungetc(c,file);
\par     *valaddr = sign * n;
\par     return(1);
\par   \} else \{
\par     /* It is. Step past it, carry on hoping for more digits */
\par     c = fgetc(file);
\par     while(c >= '0' && c <= '9') \{
\par       n = 10.0*n + c - '0';
\par       p = p*10.0;
\par       c =  fgetc(file);
\par     \}
\par     /* We've run out of digits but we have a number to give */
\par     if(c != EOF) ungetc(c,file);
\par     *valaddr = sign * n/p;
\par     return(1);
\par   \}
\par \}
\par /* Use getdouble() above but convert result to int. */
\par int 
\par getint (FILE *f, int *valaddr, int stopateol)
\par \{
\par   int r;
\par   double x;
\par   r = getdouble(f,&x,stopateol);
\par   *valaddr = (int)x;
\par   return(r);
\par \}
\par /* Use getdouble above but convert result to long. */
\par int 
\par getlong (FILE *f, long *valaddr, int stopateol)
\par \{
\par   int r;
\par   double x;
\par   r = getdouble(f,&x,stopateol);
\par   *valaddr = (long)x;
\par   return(r);
\par \}
\par 
